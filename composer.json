{
    "name": "max/simple",
    "license": "Apache-2.0",
    "description": "一款支持swoole/workerman/cli-server/FPM环境的PHP框架。",
    "type": "project",
    "keywords": [
        "maxphp",
        "swoole",
        "workerman",
        "FPM",
        "flexible",
        "php framework"
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "chengyao",
            "email": "987861463@qq.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "max/aop": "dev-master",
        "max/di": "dev-master",
        "max/http-server": "dev-master",
        "max/config": "dev-master",
        "max/event": "dev-master",
        "max/view": "dev-master",
        "max/session": "dev-master",
        "max/redis": "dev-master",
        "max/log": "dev-master",
        "max/database": "dev-master",
        "max/framework": "dev-master",
        "workerman/workerman": "^4.0",
        "vlucas/phpdotenv": "^5.4",
        "symfony/console": "^6.0"
    },
    "require-dev": {
        "swoole/ide-helper": "*",
        "symfony/var-dumper": "^6.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist"
    },
    "scripts": {
        "post-autoload-dump": [
            "rm -rf runtime/aop"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.example.env', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
